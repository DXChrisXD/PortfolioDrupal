    <div id="fh5co-logo" class="text-center">
          {{ page.logo }}
      </div>
    </header>
    <!-- Header -->
 
    <!-- Hero -->
    <aside role="complementary" id="fh5co-hero" style="background-image: url({{ banner }});">
